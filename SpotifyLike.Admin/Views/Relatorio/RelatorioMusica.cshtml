@model IEnumerable<SpotifyLike.Application.Conta.Dto.MusicaRelatorioDto>

<div>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>Id</td>
                <td>Nome</td>
                <td>Quantidade de Curtidas</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.MusicaId</td>
                    <td>@item.NomeMusica</td>
                    <td>@item.QuantidadeCurtidas</td>
                    <td></td>
                </tr>
            }

        </tbody>
    </table>
</div>
